#include<iostream>
using namespace std;
int main(){
int N;
cout<<"Enter a number"<<endl;
cin>>N;
for (int k=(N-1);k>=1;k--){
     for (int a=(k-1);a>=1;a--){
          cout<<"*";
          }
      for (int b=k;b<=(N-1);b++){
          cout<<(N-k);
          }
      cout<<""<<endl;
     }
return 0;
} 
   

